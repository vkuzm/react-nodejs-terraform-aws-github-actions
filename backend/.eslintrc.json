{
  "env": {
    "node": true,
    "commonjs": true,
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ]
  },
  "parserOptions": {
      "ecmaVersion": 2018
  }
}